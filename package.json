{
  "name": "grey-docs",
  "private": true,
  "description": "Grey Software's Docs theme",
  "license": "MIT",
  "contributors": [
    {
      "name": "NuxtJS Team"
    },
    {
      "name": "Grey Software Team"
    }
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "nuxt-ts example",
    "docs": "nuxt docs",
    "lint": "eslint --ext .js,.vue .",
    "generate-example": "yarn --cwd packages/example generate",
    "release": "lerna publish",
    "test": "yarn lint && yarn test:packages",
    "test:watch": "jest packages --watch",
    "test:packages": "jest packages -i",
    "test:packages:content": "jest packages/content -i",
    "test:packages:create-nuxt-content-docs": "jest packages/create-nuxt-content-docs"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "highlight.js": "^10.6.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "nuxt": "^2.14.12",
    "shiki": "^0.9.2",
    "shiki-twoslash": "^1.0.0"
  }
}
